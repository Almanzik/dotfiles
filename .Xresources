Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!## FONTS
!###################################################################
!##URxvt*font: xft:Inconsolata\-dz:pixelsize=14:antialias=true:hinting=true
!##URxvt*boldfont: xft:Source Code Pro:pixelsize 9:antialias=true:hinting=true

URxvt*font: xft:Cousine:pixelsize=12:antialias=true:hinting=true
URxvt*boldFont: xft:Cousine:bold:pixelsize=12:antialias=true:hinting=true

URxvt*letterSpace: -1
 
!## SCROLLBAR
!####################################################################
!# scrollbar - true (default) or false
URxvt*scrollBar: false
!# scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: false
!# scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: rxvt
 
!## COLORS
!##################################################################



*background:#222222
*foreground:#e8e9ca

*color0:    #222222
*color1:    #8b3e2f
*color2:    #526f33
*color3:    #665847
*color4:    #4a708b
*color5:    #7a378b
*color6:    #528b8b
*color7:    #999999

*color8:    #4c4c4c
*color9:    #d75f00
*color10:   #cee318
*color11:   #eee685
*color12:   #9ac0cd
*color13:   #9f79ee
